{
  "name": "@simplesmiler/taxios-sandbox",
  "private": true,
  "author": "Denis Karabaza <denis.karabaza@gmail.com>",
  "homepage": "https://github.com/simplesmiler/taxios/packages/taxios-sandbox",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/simplesmiler/taxios.git"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "generate:petstore": "ts-node ./node_modules/@simplesmiler/taxios-generate/src/bin.ts https://petstore.swagger.io/v2/swagger.json -o ./src/generated/PetStore.ts -e PetStore --named-enums --skip-additional-properties",
    "generate:petstore3": "ts-node ./node_modules/@simplesmiler/taxios-generate/src/bin.ts https://petstore3.swagger.io/api/v3/openapi.json -o ./src/generated/PetStore3.ts -e PetStore3 --named-enums --skip-additional-properties",
    "generate:named-enums": "ts-node ./node_modules/@simplesmiler/taxios-generate/src/bin.ts ./src/named-enums.yml -o ./src/generated/NamedEnums.ts -e NamedEnums --named-enums",
    "generate:snake-case": "ts-node ./node_modules/@simplesmiler/taxios-generate/src/bin.ts ./src/snake-case.yml -o ./src/generated/snake_case.ts -e snake_case --named-enums",
    "generate:query-params": "ts-node ./node_modules/@simplesmiler/taxios-generate/src/bin.ts ./src/query-params.yml -o ./src/generated/QueryParams.ts -e QueryParams",
    "generate:skip-additional-properties": "ts-node ./node_modules/@simplesmiler/taxios-generate/src/bin.ts ./src/skip-additional-properties.yml -o ./src/generated/SkipAdditionalProperties.ts -e SkipAdditionalProperties --skip-additional-properties",
    "generate:sort-fields": "ts-node ./node_modules/@simplesmiler/taxios-generate/src/bin.ts ./src/sort-fields.yml -o ./src/generated/SortFields.ts -e SortFields --sort-fields",
    "generate:ignore-min-max-items": "ts-node ./node_modules/@simplesmiler/taxios-generate/src/bin.ts src/min-max-items.yml -o src/generated/IgnoreMinMaxItems.ts -e MinMaxItems --ignore-min-max-items",
    "generate:respect-min-max-items": "ts-node ./node_modules/@simplesmiler/taxios-generate/src/bin.ts src/min-max-items.yml -o src/generated/RespectMinMaxItems.ts -e MinMaxItems"
  },
  "dependencies": {
    "@simplesmiler/taxios": "^0.2.0",
    "@simplesmiler/taxios-generate": "^0.2.0",
    "axios": "^0.27.2",
    "shx": "^0.3.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "bugs": {
    "url": "https://github.com/simplesmiler/taxios/issues"
  }
}
